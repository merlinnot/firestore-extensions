{
  "cSpell.diagnosticLevel": "Error",
  "cSpell.ignorePaths": [
    ".eslintrc",
    "*.json",
    "*.log",
    "*.yaml",
    "**/*.json",
    "coverage/**",
    "lib/**",
    "node_modules/**",
    "src/**/*.snap"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.codeAction.disableRuleComment": {
    "enable": true,
    "location": "sameLine"
  },
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.run": "onType",
  "jest.autoRun": {},
  "typescript.tsdk": "./node_modules/typescript/lib"
}
